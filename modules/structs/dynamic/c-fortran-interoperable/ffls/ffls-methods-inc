## names

# utilities (FORTRAN Constructors and Destructors)
FFLS_METHODS_FOR = ffls_methods.for

# objects
FFLS_METHODS_O = ffls_methods.o

## names relative to Makefile
FFLS_METHODS_SRC = submodules/$(FFLS_METHODS_FOR)
FFLS_METHODS_OBJ = submodules/$(FFLS_METHODS_O)

## names

# implementations
VECTOR_IINT32_FOR = Vector_int32_t_implementations.for
VECTOR_IINT64_FOR = Vector_int64_t_implementations.for
VECTOR_IMPR64_FOR = Vector_real64_t_implementations.for
VECTOR_IVECTT_FOR = Vector_vector_t_implementations.for

# objects
VECTOR_IINT32_O   = Vector_int32_t_implementations.o
VECTOR_IINT64_O   = Vector_int64_t_implementations.o
VECTOR_IMPR64_O   = Vector_real64_t_implementations.o
VECTOR_IVECTT_O   = Vector_vector_t_implementations.o

## names relative to makefile

# implementations
VECTOR_IINT32_SRC = submodules/$(VECTOR_IINT32_FOR)
VECTOR_IINT64_SRC = submodules/$(VECTOR_IINT64_FOR)
VECTOR_IMPR64_SRC = submodules/$(VECTOR_IMPR64_FOR)
VECTOR_IVECTT_SRC = submodules/$(VECTOR_IVECTT_FOR)

# objects
VECTOR_IINT32_OBJ = submodules/$(VECTOR_IINT32_O)
VECTOR_IINT64_OBJ = submodules/$(VECTOR_IINT64_O)
VECTOR_IMPR64_OBJ = submodules/$(VECTOR_IMPR64_O)
VECTOR_IVECTT_OBJ = submodules/$(VECTOR_IVECTT_O)

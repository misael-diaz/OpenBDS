# include
INC = -I../../../inc

# headers
FCONFG_H = ../../../inc/fconfig.h
CONFIG_H = ../../../inc/config.h
BDS_TYPES_H = ../../../inc/bds/types.h
BDS_PARAMS_H = ../../../inc/bds/params.h

## names

# methods
VECTOR_MINT32_FOR           = Vector_int32_t_methods.for
VECTOR_MINT64_FOR           = Vector_int64_t_methods.for
VECTOR_METR64_FOR           = Vector_real64_t_methods.for
VECTOR_MVECTT_FOR           = Vector_vector_t_methods.for
VECTOR_OBLIVI_FOR           = Vector_type_oblivious_methods.for
VECTOR_ERASE_FOR            = Vector_int32_t_erase_methods.for
VECTOR_INT32_PUSH_BACK_FOR  = Vector_int32_t_push_back_methods.for
VECTOR_INT64_PUSH_BACK_FOR  = Vector_int64_t_push_back_methods.for
VECTOR_REAL64_PUSH_BACK_FOR = Vector_real64_t_push_back_methods.for
VECTOR_VECTOR_PUSH_BACK_FOR = Vector_vector_t_push_back_methods.for

# objects
VECTOR_MINT32_O             = Vector_int32_t_methods.o
VECTOR_MINT64_O             = Vector_int64_t_methods.o
VECTOR_METR64_O             = Vector_real64_t_methods.o
VECTOR_MVECTT_O             = Vector_vector_t_methods.o
VECTOR_OBLIVI_O             = Vector_type_oblivious_methods.o
VECTOR_ERASE_O              = Vector_int32_t_erase_methods.o
VECTOR_INT32_PUSH_BACK_O    = Vector_int32_t_push_back_methods.o
VECTOR_INT64_PUSH_BACK_O    = Vector_int64_t_push_back_methods.o
VECTOR_REAL64_PUSH_BACK_O   = Vector_real64_t_push_back_methods.o
VECTOR_VECTOR_PUSH_BACK_O   = Vector_vector_t_push_back_methods.o


## names relative to makefile

# methods
VECTOR_MINT32_SRC           = submodules/$(VECTOR_MINT32_FOR)
VECTOR_MINT64_SRC           = submodules/$(VECTOR_MINT64_FOR)
VECTOR_METR64_SRC           = submodules/$(VECTOR_METR64_FOR)
VECTOR_MVECTT_SRC           = submodules/$(VECTOR_MVECTT_FOR)
VECTOR_OBLIVI_SRC           = submodules/$(VECTOR_OBLIVI_FOR)
VECTOR_ERASE_SRC            = submodules/$(VECTOR_ERASE_FOR)
VECTOR_INT32_PUSH_BACK_SRC  = submodules/$(VECTOR_INT32_PUSH_BACK_FOR)
VECTOR_INT64_PUSH_BACK_SRC  = submodules/$(VECTOR_INT64_PUSH_BACK_FOR)
VECTOR_REAL64_PUSH_BACK_SRC = submodules/$(VECTOR_REAL64_PUSH_BACK_FOR)
VECTOR_VECTOR_PUSH_BACK_SRC = submodules/$(VECTOR_VECTOR_PUSH_BACK_FOR)

# objects
VECTOR_MINT32_OBJ           = submodules/$(VECTOR_MINT32_O)
VECTOR_MINT64_OBJ           = submodules/$(VECTOR_MINT64_O)
VECTOR_METR64_OBJ           = submodules/$(VECTOR_METR64_O)
VECTOR_MVECTT_OBJ           = submodules/$(VECTOR_MVECTT_O)
VECTOR_OBLIVI_OBJ           = submodules/$(VECTOR_OBLIVI_O)
VECTOR_ERASE_OBJ            = submodules/$(VECTOR_ERASE_O)
VECTOR_INT32_PUSH_BACK_OBJ  = submodules/$(VECTOR_INT32_PUSH_BACK_O)
VECTOR_INT64_PUSH_BACK_OBJ  = submodules/$(VECTOR_INT64_PUSH_BACK_O)
VECTOR_REAL64_PUSH_BACK_OBJ = submodules/$(VECTOR_REAL64_PUSH_BACK_O)
VECTOR_VECTOR_PUSH_BACK_OBJ = submodules/$(VECTOR_VECTOR_PUSH_BACK_O)
